@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<header>
    <h2 class="text-danger d-flex align-items-center">
        <i class="bi bi-person-lock me-2"></i> @ViewData["Title"]
    </h2>
    <p class="text-danger">This account has been locked out, please ask for administrator support.</p>
</header>
